{
  "name": "immersion",
  "version": "1.0.0",
  "description": "plop",
  "main": "src/index.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "node_modules/.bin/tsc",
    "start": "node --require dotenv-expand/config --experimental-specifier-resolution=node dist/index.js",
    "start:dev": "pnpm build && pnpm start",
    "test": "node --experimental-modules --experimental-vm-modules node_modules/jest/bin/jest.js --experimental-specifier-resolution=node --config jest.config.ts --detectOpenHandles",
    "lint": "pnpm -w run lint:back",
    "prettier": "pnpm -w run prettier:back"
  },
  "dependencies": {
    "@serenity-dev/http-client": "^0.0.15",
    "@types/supertest": "^2.0.12",
    "dotenv": "^16.0.1",
    "dotenv-expand": "^8.0.3",
    "express": "^4.18.1",
    "pino": "^8.3.1",
    "pino-http": "^8.2.0",
    "pino-pretty": "^8.1.0",
    "prom-client": "^14.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.2",
    "@types/pino-http": "^5.8.1",
    "jest": "^28.1.3",
    "shared": "workspace:shared",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
